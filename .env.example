# Copy this file to `.env` and fill in the values before running the app.

NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=public-anon-key
SUPABASE_SERVICE_ROLE_KEY=service-role-key

# LLM providers (optional, but at least one should be set for generation endpoints)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Defaults for the conversational generator
FLOWLEARN_DEFAULT_MODEL=gpt-4o-mini
FLOWLEARN_FALLBACK_MODEL=claude-3-5-sonnet
